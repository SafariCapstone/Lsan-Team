{
    "manifest_version": 3,
    "name": "Lsan Chrome Extension",
    "version": "0.1.0",
    "description": "A Chrome extension for Lsan service",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "storage",
        "scripting"
    ],

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
}